    "@types/yargs": "^15.0.10",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "amplify-graphql-docs-generator": "^2.1.16",
    "change-case": "^4.1.1",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "inquirer": "^7.3.3",
    "prettier": "^2.1.2",
    "yargs": "^16.1.1"
  }
}
