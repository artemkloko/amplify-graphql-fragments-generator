{
  "name": "amplify-graphql-fragments-generator",
  "version": "0.0.1",
  "description": "amplify graphql docs with fragments generator plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/artemkloko/amplify-graphql-fragments-generator.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "appsync",
    "aws",
    "fragments",
    "statements"
  ],
  "engines": {
    "node": ">=10.17.0"
  },
  "bin": {
    "amplify-graphql-fragments-generator": "./bin/cli"
  },
  "scripts": {
